class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
        int i=matrix[0].size()-1;
        int row=matrix.size()-1;
        int j=0;
        while(i>=0&&j<=row)
        {
            if(matrix[j][i]>target)
                 i--;
            else if(matrix[j][i]<target)
            {
                j++;
            } 
            else
              return true;    
        }

        return false;

    }
};
